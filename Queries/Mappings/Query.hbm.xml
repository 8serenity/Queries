<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="Queries" namespace="Queries.Models">
  <class name="Query" table="Queries" dynamic-update="true" >
    <cache usage="read-write"/>
    
    <id name="QueryID" column="QueryID" type="int">
      <generator class="native" />
    </id>

    <property name="QueryText"  />
    <property name="DateWritten" />
    <property name="QueryStatus" />
    <property name="UserId" />
  </class>
</hibernate-mapping>